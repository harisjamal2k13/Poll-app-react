  "dependencies": {
    "bootstrap": "^4.0.0-beta.3",
    "firebase": "^4.8.2",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-easy-chart": "^1.0.0",
    "react-radio-buttons": "^1.2.1",
    "react-redux": "^5.0.6",
